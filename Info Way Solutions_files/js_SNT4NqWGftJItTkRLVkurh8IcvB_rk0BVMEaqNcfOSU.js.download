_slideShowThumb = function () {
  if (jQuery('#slideshowClients').length == 0) return;
  _ctrThumb = _total = _timer = 0;
  _int = 400;
  _interval = 3000;
  _total = jQuery('#slideshowClients > div').size();
  jQuery('#slideshowClients > div').css({
    'display': 'none'
  });
  jQuery('#slideshowClients > div').eq(_ctrThumb).fadeIn(_int);
  _timer = setInterval(function () {
    jQuery('#slideshowClients > div').eq(_ctrThumb).fadeOut(_int, function () {
      _ctrThumb++;
      if (_ctrThumb >= _total) _ctrThumb = 0;
      jQuery('#slideshowClients > div').eq(_ctrThumb).fadeIn(_int);
    });
  }, _interval);
}



