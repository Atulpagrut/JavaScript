_slide_init = function() {
    //Delcare variables
    _slide_timer = _slide_ctr = _slide_tot = 0;
    _slideInterval = 7000;
    _slideAnim = 700;

    //Resize Banners according giving resolutions
    if ($('.banner#banner_slideshow').length === 0)
        return;
    $('#banner_slideshow > .slides, #banner_slideshow > .slides > div').css('height', $('.banner#banner_slideshow').outerHeight()); ///Height Sizing
    $(window).on('load resize', function() {
        $('#banner_slideshow > .slides, #banner_slideshow > .slides > div').css('height', $('.banner#banner_slideshow').outerHeight());
        $('#banner_slideshow > .slides > div').each(function() { ///Keep the Inactive Slides at bay
            if (!$(this).hasClass('active')) {
                $(this).css({'left': $('#banner_slideshow').width()});
                $(this).find('.bannerCaption').css('margin-left', -$('#banner_slideshow').width());
            }
        });
    });

    ///Initial settings
    _slide_tot = $('#banner_slideshow > .slides > div').size() - 1;
    $('#banner_slideshow > .slides > div').each(function(i) { ///Keep the Inactive Slides at bay
        if (!$(this).hasClass('active')) {
            $(this).css({'left': $('#banner_slideshow').width()});
            $(this).find('.bannerCaption').css('margin-left', -$('#banner_slideshow').width());
        }
        ///Insert Bullets for nav
        $('#banner_slideshow > .nav').append('<span>' + (i + 1) + '</span>');
    });
    $('#banner_slideshow > .nav > span:eq(0)').addClass('active');


    ////Function for timer/Slideshow
    _slide = function() {
        _arg = arguments[0];
        if (!isNaN(_arg)) {
