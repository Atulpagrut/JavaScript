jQuery(document).ready(function(){

	jQuery('#clients-speak-cycle').cycle({                            
            timeout:6000,
            delay:  -2000 
            //speed:  1
      }).cycle("resume").hover(function() {
            jQuery(this).cycle('pause');
      },function(){
            jQuery(this).cycle('resume');
      });
	
	
	if(jQuery("#scroller").length>0)jQuery("#scroller").simplyScroll({orientation:'vertical',customClass:'vert'});
	jQuery('#logo_cycle').cycle({
				fx:     'fade',
				timeout: 3000,
				delay:  -2000
			});
	
	jQuery("ul#slides").cycle({
		fx: 'fade',
		pause: 1,
		prev: '#prev',
		next: '#next'
	});
	
	/* Validation for contact form's contact number in home page */
	jQuery('#edit-submitted-contact-no--2').keyup(function() {
	  var foo = jQuery(this).val().split("-").join(""); // remove hyphens
	  foo = foo.match(new RegExp('.{1,4}$|.{1,3}', 'g')).join("-");
	  jQuery(this).val(foo);  
	});
	
	
});


